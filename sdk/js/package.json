    "url": "git+https://github.com/quantum-blockchain/quantum-sdk-js.git"
  },
  "bugs": {
    "url": "https://github.com/quantum-blockchain/quantum-sdk-js/issues"
  },
  "homepage": "https://github.com/quantum-blockchain/quantum-sdk-js#readme",
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.0",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "eslint": "^8.37.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.0.5",
    "ts-loader": "^9.4.2",
